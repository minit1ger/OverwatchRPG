<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="SubmitData" prefHeight="45.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.SubmitDataController">
   <children>
      <ButtonBar layoutX="186.0" layoutY="50.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="clear" mnemonicParsing="false" onMouseClicked="#clearForm" onMousePressed="#clearForm" text="Clear" />
            <Button fx:id="submit" mnemonicParsing="false" onAction="#submitForm" text="Submit" />
        </buttons>
      </ButtonBar>
      <Label layoutX="14.0" layoutY="23.0" text="Saved To:" />
      <TextField fx:id="file_path" layoutX="68.0" layoutY="19.0" prefHeight="25.0" prefWidth="318.0" />
   </children>
</AnchorPane>
